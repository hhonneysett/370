@model LibraryAssistantApp.Models.EmployeeAddModel

<table class="table">
    <thead>
        <tr>
            <th>
                Action
            </th>
            <th>
                Create
            </th>
            <th>
                Read
            </th>
            <th>
                Update
            </th>
            <th>
                Delete
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.role_action)
        {
            <tr id="@item.Action_ID">
                <td>
                    @item.Action.Action_Name
                </td>
                <td>
                    @item.Create_Ind
                </td>
                <td>
                    @item.Read_Ind
                </td>
                <td>
                    @item.Update_Ind
                </td>
                <td>
                    @item.Delete_Ind
                </td>
            </tr>
        }
    </tbody>
</table>

